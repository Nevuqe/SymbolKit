{
  "name": "SymbolKit",
  "version": "1.0.0",
  "description": "SymbolKit, an open-source iconography library consisting of 1,300+ SVG icons.",
  "type": "module",
  "files": [
    "css",
    "icons"
  ],
  "scripts": {
    "lint": "eslint ./bin/build.js",
    "lint:fix": "npm run lint -- --fix",
    "build": "node ./bin/build.js",
    "prepublish-all": "node ./bin/prepublish.js",
    "dist": "pnpm -r --filter './packages/**' run dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nevuqe/symbolkit.git"
  },
  "keywords": [
    "free",
    "icons",
    "library",
    "svg",
    "react",
    "css"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/nevuqe/symbolkit/issues"
  },
  "homepage": "https://developer.nevuqe.com",
  "devDependencies": {
    "@atomist/yaml-updater": "^1.0.2",
    "@svgr/cli": "^5.5.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "^5.1.1",
    "generate-template-files": "^3.2.1",
    "listr2": "^3.14.0",
    "prettier": "^2.8.7",
    "semver": "^7.3.8"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "^17.0.29"
    }
  }
}